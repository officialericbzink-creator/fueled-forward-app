{
  "cli": {
    "version": ">= 3.15.1",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "APP_ENV": "preview"
      },
      "environment": "preview",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "APP_ENV": "production"
      },
      "environment": "production",
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "../google-service-account-key.json",
        "track": "production"
      },
      "ios": {
        "appleId": "eric.zink@gmail.com",
        "ascAppId": "6757867912"
      }
    },
    "preview": {
      "android": {
        "serviceAccountKeyPath": "../google-service-account-key.json",
        "track": "internal"
      },
      "ios": {
        "appleId": "eric.zink@gmail.com",
        "ascAppId": "6757867912"
      }
    }
  }
}

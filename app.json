{
  "version": "0.0.2",
  "orientation": "portrait",
  "userInterfaceStyle": "automatic",
  "icon": "./assets/images/ff-icon-1024.png",
  "updates": {
    "fallbackToCacheTimeout": 0,
    "url": "https://u.expo.dev/f219f452-ee86-4e74-84b3-ecfcfdadbd17"
  },
  "newArchEnabled": true,
  "jsEngine": "hermes",
  "assetBundlePatterns": ["**/*"],
  "android": {
    "icon": "./assets/images/ff-icon-1024.png",
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/ff-icon-1024.png",
      "backgroundImage": "./assets/images/ff-icon-1024.png"
    },
    "allowBackup": false,
    "edgeToEdgeEnabled": true,
    "package": "com.fueledforwardapp",
    "permissions": ["android.permission.RECORD_AUDIO"]
  },
  "ios": {
    "icon": "./assets/images/ff-icon-1024.png",
    "supportsTablet": true,
    "bundleIdentifier": "com.fueledforwardapp",
    "privacyManifests": {
      "NSPrivacyAccessedAPITypes": [
        {
          "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
          "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
        },
        {
          "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
          "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
        }
      ]
    },
    "infoPlist": {
      "ITSAppUsesNonExemptEncryption": false
    }
  },
  "web": {
    "favicon": "./assets/images/app-icon-web-favicon.png",
    "bundler": "metro"
  },
  "plugins": [
    "expo-localization",
    "expo-font",
    "expo-web-browser",
    "expo-secure-store",
    [
      "expo-image-picker",
      {
        "photosPermission": "Allow $(PRODUCT_NAME) to access your photos"
      }
    ],
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/ff-splash-screen.png",
        "resizeMode": "contain",
        "imageWidth": "100%",
        "backgroundColor": "#000000"
      }
    ],
    [
      "react-native-edge-to-edge",
      {
        "android": {
          "parentTheme": "Light",
          "enforceNavigationBarContrast": false
        }
      }
    ]
  ],
  "experiments": {
    "tsconfigPaths": true
  },
  "extra": {
    "ignite": {
      "version": "11.1.3"
    }
  },
  "owner": "fueled-forward",
  "runtimeVersion": {
    "policy": "appVersion"
  }
}
